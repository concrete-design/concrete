{
  "name": "concrete",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^13.1.1",
    "turbo": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.1.4"
}

{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint:fix": {
      "cache": false,
      "outputs": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.mdx"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "start": {
      "cache": false
    }
  }
}
